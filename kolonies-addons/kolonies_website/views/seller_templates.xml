<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <template id="kolonies_sellers_list_item" inherit_id="odoo_marketplace.seller_list_item" name="Kolonies Sellers List Item">
      <xpath expr="//div[@id='seller_name']" position="inside">
        <t t-if="seller_obj.sudo().is_online()">
          <i class="fa fa-circle" style="color: green;"/>
        </t>
        <t t-else="">
          <i class="fa fa-circle" style="color: red;"/>
        </t>
      </xpath>
    </template>

  </data>
</odoo>